{
	"name": "@gwigz/homunculus-terminal",
	"description": "A third-party terminal client for interacting with Linden Lab's virtual world \"Second Life\"",
	"version": "0.0.0-alpha.9",
	"author": "gwigz <gwigz@icloud.com>",
	"bugs": "https://github.com/gwigz/homunculus/issues",
	"homepage": "https://github.com/gwigz/homunculus#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gwigz/homunculus.git"
	},
	"keywords": [
		"bot",
		"life",
		"second",
		"secondlife"
	],
	"license": "MIT",
	"files": [
		"dist"
	],
	"main": "./dist/index.mjs",
	"bin": {
		"homunculus": "./dist/index.mjs"
	},
	"scripts": {
		"dev": "bun run src/index.ts",
		"lint": "biome lint .",
		"prebuild": "rm -rf dist",
		"build": "bun build ./src/index.ts --outfile ./dist/index.mjs --target node --external blessed --external sqlite3"
	},
	"dependencies": {
		"blessed": "^0.1.81",
		"sqlite3": "^5.1.7"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.0-beta.5",
		"@gwigz/homunculus-core": "workspace:*",
		"@types/blessed": "^0.1.25",
		"@types/node": "^22.15.20",
		"dotenv": "^16.5.0",
		"meow": "^13.2.0"
	}
}
