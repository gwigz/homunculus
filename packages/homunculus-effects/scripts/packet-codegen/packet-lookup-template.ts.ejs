 * This file is used to help our packet serialization and deserialization
 * process, and to create new packets on the fly.
 *
 * ⚠️ Do not edit this file manually, it is generated by the `codegen` script!
 *
 * @see {@link http://wiki.secondlife.com/wiki/Message_Layout}
 */

import { type PacketMetadata, packets } from "~/network"

export class PacketLookup {
	private static list: Record<number, PacketMetadata> | null = null

	public static find(id: number) {
		if (!PacketLookup.list) {
			PacketLookup.list = {
<% packets.forEach(function(packet) { -%>
				<%= packet.id %><%= packet.frequency %>: packets.<%= packet.camelCaseName %>Metadata,
<% }); -%>
			}
		}

		return PacketLookup.list[id]
	}
}
